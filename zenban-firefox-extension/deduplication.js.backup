// Simple deduplication for bookmark messages
const processedBookmarks = new Set();

function isDuplicateBookmark(data) {
    // Create unique key from bookmark data
    const key = `${data.url}_${data.timestamp}`;
    
    if (processedBookmarks.has(key)) {
        return true;
    }
    
    // Add to set and clean old entries after 5 seconds
    processedBookmarks.add(key);
    setTimeout(() => processedBookmarks.delete(key), 5000);
    
    return false;
}

// Export for use in app
window.isDuplicateBookmark = isDuplicateBookmark;
